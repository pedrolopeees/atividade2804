import java.util.*;

public class atv11 {
  public static void main(String args[]) {
      
    int[] vetor = {3, 9, 2, 10, 18, 6, 4, 8, 20, 24};
    
    int soma = 0;
        
        for (int i = 0; i < vetor.length; i++){
        soma = soma + vetor[i];
        }
        
        System.out.println("A soma Ã©: "+soma);
    
    }
}
