public class atv04 {
    public static void main(String args[]) {
        int a, b, c, i = 1, n;
        n = 10;
        a = 0;
        b = 1;
        System.out.print(a);

        while(i< n) {
            c = a + b;
            System.out.print(", " + c);
            a = b;
            b = c;
            i++;
        }
    }
}
